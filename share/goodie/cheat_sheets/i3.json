            "key": "[Mod1] [Enter]"
        }, {
            "val": "Focus left",
            "key": "[Mod1] ([j]/[Left])"
        }, {
            "val": "Focus down",
            "key": "[Mod1] ([k]/[Down])"
        }, {
            "val": "Focus up",
            "key": "[Mod1] ([l]/[Up])"
        }, {
            "val": "Focus right",
            "key": "[Mod1] ([;]/[Right])"
