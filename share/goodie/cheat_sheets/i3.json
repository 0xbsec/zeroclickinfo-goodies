            "val": "Move window left",
            "key": "[Mod1] [Shift] ([j]/[Left])"
        }, {
            "val": "Move window down",
            "key": "[Mod1] [Shift] ([k]/[Down])"
        }, {
            "val": "Move window up",
            "key": "[Mod1] [Shift] ([l]/[Up])"
        }, {
            "val": "Move window right",
            "key": "[Mod1] [Shift] ([;]/[Right])"
        }, {
            "val": "Kill a window",
            "key": "[Mod1] [Shift] [q]"
