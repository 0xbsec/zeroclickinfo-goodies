            "val": "Open application launcher",
            "key": "[Mod1] [d]"
        }],
        "Using workspaces": [{
            "val": "Switch to another workspace",
            "key": "[Mod1] <number>"
        }],
        "Restarting i3 inplace": [{
            "val": "Restart i3 inplace",
            "key": "[Mod1] [Shift] [r]"
        }],
        "Exiting i3": [{
            "val": "Exit i3",
            "key": "[Mod1] [Shift] [e]"
        }]
