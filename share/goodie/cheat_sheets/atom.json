{
    "id":"github_atom_cheat_sheet",
    "name":"Atom",
    "metadata":{
        "sourceName":"Bugsnag",
        "sourceUrl":"https://bugsnag.com/blog/atom-editor-cheat-sheet"
    },
    "section_order":[
        "General",
        "File Navigation",
        "View/Window Manipulation",
        "Find",
        "Folding",
        "Word Manipulation",
        "Line Manipulation",
        "Selection"
    ],
    "sections":{
        "General":[
            {
                "key":"[shift] [⌘] [p]",
                "val":"Toggle command palette"
            },
            {
                "key":"[⌘] [/]",
                "val":"Toggle line / Selection comment"
            },
            {
                "key":"[ctrl] [space]",
                "val":"Show available auto-completions"
            }
        ],
        "File Navigation":[
            {
                "key":"[⌘] [p]",
                "val":"Toggle file finder"
            },
            {
                "key":"[ctrl] [g]",
                "val":"Goto line"
            },
            {
                "key":"[⌘] [r]",
                "val":"Goto symbol"
            },
            {
                "key":"[shift] [⌘] { / }",
                "val":"Previous/next file"
            },
            {
                "key":"[⌘] [F2]",
                "val":"Set bookmark"
            },
            {
                "key":"F2",
                "val":"Goto next bookmark"
            }
        ],
        "View/Window Manipulation":[
            {
                "key":"[⌘] [\\]",
                "val":"Toggle tree-view sidebar"
            },
            {
                "key":"[ctrl] [⌘] [f]",
                "val":"Toggle fullscreen"
            },
            {
                "key":"[⌘] [k]",
                "val":"Split pane left/right/up/down"
            }
        ],
        "Find":[
            {
                "key":"[⌘] [f]",
                "val":"Find in file"
            },
            {
                "key":"[⌘] [g]",
                "val":"Find next"
            },
            {
                "key":"[shift] [⌘] [f]",
                "val":"Find in project"
            },
            {
                "key":"[⌘] [e]",
                "val":"Use selection for find"
            }
        ],
        "Folding":[
            {
                "key":"[ctrl] [⌘] [/]",
                "val":"Fold/unfold current code block"
            },
            {
                "key":"[option] [shift] [⌘] [/]",
                "val":"Fold/unfold all code blocks"
            },
            {
                "key":"[⌘] [1..9]",
                "val":"Fold code blocks at depth 1-9 "
            }
        ],
        "Word Manipulation":[
            {
                "key":"[ctrl] [t]",
                "val":"Transpose characters either side of cursor"
            },
            {
                "key":"[option] [backspace]",
                "val":"Delete text to beginning of word"
            },
            {
                "key":"[option] [delete]",
                "val":"Delete text to end of word"
            }
        ],
        "Line Manipulation":[
            {
                "key":"[⌘] ] / [",
                "val":"Indent/outdent current line"
            },
            {
                "key":"[⌘] [enter]",
                "val":"Insert new line after current line"
            },
            {
                "key":"[⌘] [shift] [enter]",
                "val":"Insert new line before current line"
            },
            {
                "key":"[ctrl] [shift] [k]",
                "val":"Delete current line"
            },
            {
                "key":"[ctrl] [⌘] [(up/down)]",
                "val":"Move current line up/down"
            },
            {
                "key":"[shift] [⌘] [d]",
                "val":"Duplicate current line"
            },
            {
                "key":"[⌘] [j]",
                "val":"Join current and next lines"
            }
        ],
        "Selection":[
            {
                "key":"[⌘] [d]",
                "val":"Select current word/token"
            },
            {
                "key":"[⌘] [l]",
                "val":"Select current line"
            },
            {
                "key":"[option] [shift] [(left/right)]",
                "val":"Select to beginning/end of word"
            },
            {
                "key":"[shift] [⌘] [(left/right)]",
                "val":"Select to first/last character of line"
            },
            {
                "key":"[shift] [⌘] [(up/down)]",
                "val":"Select to top/bottom of document"
            }
        ]
    }
}
