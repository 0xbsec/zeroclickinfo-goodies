{
    "id":"elementary_cheat_sheet",
    "name":"elementary OS",
    "metadata":{
        "sourceName":"Lme Linux",
        "sourceUrl":"http://lmelinux.net/2014/07/26/elementary-os-keyboard-shortcuts/"
    },
    "section_order":[
        "General"
    ],
    "sections":{
        "General":[
            {
                "val":"App Launcher",
                "key":"[⌘] [Space]"
            },
            {
                "val":"Window Switcher",
                "key":"[Alt] [Tab]"
            },
            {
                "val":"Switch Windows Backwards",
                "key":"[⇧] [Alt] [Tab]"
            },
            {
                "val":"Window overview",
                "key":"[⌘] [W]"
            },
            {
                "val":"Window Overview (Across All Workspaces)",
                "key":"[⌘] [A]"
            },
            {
                "val":"Switch Workspace",
                "key":"[⌘] ([Left]/[Right])"
            },
            {
                "val":"Workspace Overview",
                "key":"[⌘] [S]"
            },
            {
                "val":"New Workspace",
                "key":"[⌘] [O]"
            },
            {
                "val":"Will switch to a specific workspace (If it currently exists)",
                "key":"[⌘] [1..9]"
            },
            {
                "val":"Will switch to the first workspace",
                "key":"[⌘] [Home]"
            },
            {
                "val":"Will switch to the last workspace",
                "key":"[⌘] [End]"
            },
            {
                "val":"Move Window with Workspace",
                "key":"[⇧] [⌘] ([Left]/[Right])"
            },
            {
                "val":"Snap Window to Half of Workspace",
                "key":"[Ctrl] [⌘] ([Left]/[Right])"
            },
            {
                "val":"Maximize/Unmaximize Window",
                "key":"[ctrl] [⌘] ([Left]/[Right])"
            },
            {
                "val":"Lock Screen",
                "key":"[⌘] [L]"
            },
            {
                "val":"Move Window",
                "key":"[⌘] [Click] [drag]"
            },
            {
                "val":"Resize Window",
                "key":"[⌘] [Right Click] [drag]"
            }
        ]
    }
}
