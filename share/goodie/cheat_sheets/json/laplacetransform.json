            },
            {
                "key": "tcos(at)",
                "val": "[(s^2)-(a^2)]/[[(s^2)+(a^2)]^2]"
            },
            {
                "key": "sinh(at)",
                "val": "a/[(s^2)-(a^2)]"
            },
            {
                "key": "cosh(at)",
                "val": "s/[(s^2)-(a^2)]"  
            },
            {
                "key": "[e^(at)]sin(bt)",
