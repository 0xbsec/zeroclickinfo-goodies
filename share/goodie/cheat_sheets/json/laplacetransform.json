    "sections": {
        "Common Transforms" : [
            {
                "key": "L( 1 ) = 1/s"  
            },
            {
                "key": "L( t^n ) = (n!)/[s^(n+1)]"
            },
            {
                "key": "L( e^(at) ) = 1/(s-a)"
            },
            {
                "key": "L( sin(at) ) = a/[(s^2)+(a^2)]"
            },
            {
                "key": "L( cos(at) ) = s/[(s^2)+(a^2)]"
            },
            {
                "key": "L( tsin(at) ) = (2as)/[[(s^2)+(a^2)]^2]"
            },
            {
                "key": "L( tcos(at) ) = [(s^2)-(a^2)]/[[(s^2)+(a^2)]^2]"
            },
            {
                "key": "L( sinh(at) ) = a/[(s^2)-(a^2)]"
            },
            {
                "key": "L( cosh(at) ) = s/[(s^2)-(a^2)]"
            },
            {
                "key": "L( [e^(at)]sin(bt) ) = b/[[(s-a)^2]+(b^2)]"
            },
            {
                "key": "L( [e^(at)]cos(bt) ) = (s-a)/[[(s-a)^2]+(b^2)]"
            },
            {
                "key": "L( [e^(at)]sinh(bt) ) = b/[[(s-a)^2]-(b^2)]"
            },
            {
                "key": "L( [e^(at)]cosh(bt) ) = (s-a)/[[(s-a)^2]-(b^2)]"
            },
            {
                "key": "L( [t^n][e^(at)] ) = (n!)/[(s-a)^(n+1)]"
            }
        ],
        "Identities": [
            {
                "key": "F(s) = L{f(t)}"
            },
            {
                "key": "f(t) = (L^-1){F(s)}"
            }
        ]
    }
}
