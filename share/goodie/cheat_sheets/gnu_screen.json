    "section_order": ["Session Commands (from the command line)", "Copying and Scrolling", "Main Comands", "Window Commands"],
    "sections": {
        "Session Commands (from the command line)": [
        {
            "val": "screen",
            "key": "Start a new session"
        },
        {
            "val": "screen -S <name>",
            "key": "Start a new named session"
        },
        {
            "val": "screen -ls",
            "key": "List sessions"
        },
        {
            "val": "screen -r [name]",
            "key": "Re-attach session"
        }
        ],
        "Copying and Scrolling": [
        {
            "val": "Ctrl-a, [",
            "key": "Copy mode"
        },
        {
            "val": "Ctrl-a, space",
            "key": "Start copy"
        },
        {
            "val": "Ctrl-a, space",
            "key": "End copy"
        },
        {
            "val": "Ctrl-a, ]",
            "key": "Paste"
        },
        {
            "val": "Ctrl-a, u",
            "key": "Scroll up"
        },
        {
            "val": "Ctrl-a, d",
            "key": "Scroll down"
        },
        {
            "val": "Ctrl-a, escape",
            "key": "End copy mode"
        }
        ],
        "Main Commands": [
        {
            "val": "Ctrl-a, ?",
            "key": "Shortcuts menu"
        },
        {
